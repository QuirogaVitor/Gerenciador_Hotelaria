<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.TelaLogin">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label alignment="CENTER" layoutX="236.0" layoutY="49.0" prefHeight="29.0" prefWidth="146.0" text="LOGIN" />
            <Label alignment="CENTER" layoutX="228.0" layoutY="108.0" prefHeight="23.0" prefWidth="63.0" text="Usuario" />
            <Label alignment="CENTER" layoutX="235.0" layoutY="177.0" prefHeight="23.0" prefWidth="50.0" text="Senha" />
            <TextField fx:id="campoUsuario" layoutX="235.0" layoutY="141.0" />
            <TextField fx:id="campoSenha" layoutX="235.0" layoutY="200.0" />
            <Button fx:id="botaoEntrarLogin" layoutX="236.0" layoutY="247.0" mnemonicParsing="false" onAction="#FazerLogin" text="Entrar" />
            <Button fx:id="BotaoLimparLogin" layoutX="329.0" layoutY="247.0" mnemonicParsing="false" onAction="#LimparLogin" text="Limpar" />
            <Button fx:id="botaoIrTelaCadastro" layoutX="236.0" layoutY="298.0" mnemonicParsing="false" onAction="#chamarTelaCadastro" text="Fazer Cadastro" />
         </children>
    </AnchorPane>
  </children>
</VBox>
